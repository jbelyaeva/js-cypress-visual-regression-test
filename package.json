  "devDependencies": {
    "@shelex/cypress-allure-plugin": "^2.3.4",
    "cypress": "^6.4.0",
    "cypress-localstorage-commands": "^1.4.0",
    "cypress-xpath": "^1.6.1"
  },
  "dependencies": {
    "cypress-visual-regression": "^1.5.6",
    "mongodb": "^3.6.3"
  }
}
